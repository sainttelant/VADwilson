{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "VAD nuScenes Converter",
      "type": "python",
      "request": "launch",
      "program": "${workspaceFolder}/tools/data_converter/vad_nuscenes_converter.py",
      "args": [
        "nuscenes",
        "--root-path", "./data",
        "--out-dir", "./data/",
        "--extra-tag", "vad_nuscenes",
        "--version", "v1.0",
        "--canbus", "./data"
      ],
      "console": "integratedTerminal",
      "justMyCode": false,
      "env": {
        "PYTHONPATH": "${workspaceFolder}"
      }
    }
  ]
}